ggsurvplot(fit, data = colon,
size = 0.5,
linetype = "strata",
legend = c(0.3, 0.2),
break.time.by = 500,
pval = TRUE,
palette = "lancet",
risk.table = TRUE,
risk.table.y.text.col = TRUE
)
